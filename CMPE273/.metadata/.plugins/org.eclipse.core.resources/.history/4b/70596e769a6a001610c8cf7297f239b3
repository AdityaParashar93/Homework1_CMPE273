function encry()
{
	var msg=document.getElementById("msg").value;
	var i;
	var encrypted_msg="";
	var decrypted_msg="";
	for(i=msg.length;i>=0;i--)
	{
		encrypted_msg=encrypted_msg+msg.charAt(i);
	};
	for(i=encrypted_msg.length;i>=0;i--)
	{
		temp2=temp2+msg.charAt(i);
	};
	document.write(temp2);
};