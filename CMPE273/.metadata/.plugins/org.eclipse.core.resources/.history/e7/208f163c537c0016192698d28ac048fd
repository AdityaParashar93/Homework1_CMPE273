<h2>Create Object from JSON String</h2>

<p id="demo1"></p>
<p id="demo2"></p>
<p id="demo3"></p>

<script>
var text = '{"student":[' +
'{"firstName":"Jay","lastName":"Nirgudkar" },' +
'{"firstName":"Aditya","lastName":"Parashar" },' +
'{"firstName":"Sagar","lastName":"Mane" }]}';

obj = JSON.parse(text);

document.getElementById("demo1").innerHTML =obj.student[1].firstName + " " + obj.student[1].lastName;


</script>

