<p id="demo"></p>

<script>
var text = '{"Student ":[' +
'{"firstName":"John","lastName":"Doe" },' +
'{"firstName":"Anna","lastName":"Smith" },' +
'{"firstName":"Peter","lastName":"Jones" }]}';

obj = JSON.parse(text);
document.getElementById("demo").innerHTML =
obj.employees[1].firstName + " " + obj.employees[1].lastName+obj.employees[2].firstName + " " + obj.employees[2].lastName+obj.employees[3].firstName + " " + obj.employees[3].lastName;
</script>

