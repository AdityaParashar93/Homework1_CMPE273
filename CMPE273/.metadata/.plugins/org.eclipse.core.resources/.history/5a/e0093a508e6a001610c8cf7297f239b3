function genAverage()
{
	var ave=new Object();
	ave.name=document.getElementById("name").value;
	ave.surname=document.getElementById("surname").value;
	ave.dist_1=document.getElementById("dist_1").value;
	ave.dist_2=document.getElementById("dist_2").value;
	ave.dist_3=document.getElementById("dist_3").value;
	ave.time_1=document.getElementById("time_1").value;
	ave.time_2=document.getElementById("time_2").value;
	ave.time_3=document.getElementById("time_3").value;
	int temp=10;//(parseInt(dist_1)+parseInt(dist2)+parseInt(dist_3))/(parseInt(time_1)+parseInt(time_2)+parseInt(time_3));
	var temp1=temp.toString();
	document.write(ave.name+"\t"+ave.surname+"\t"+temp1.toString());//(ave.dist_1).toString()+"\t"+(ave.dist_2).toString()+"\t"+(ave.dist_3).toString()+"\t");
};