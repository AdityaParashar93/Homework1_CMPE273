import java.io.*;
import java.util.*;
public class Collections 
{
	LinkedList<String> l1=new LinkedList<String>();
	
	int repair()
	{
		int choice=1;
		BufferedReader br=new BufferedReader(new InputStreamReader(System.in));
		while(choice==1)
		{
			System.out.println("Enter the name of the Customer's car to be repaired");
			String name;
			name=br.readLine();
			l1.add(name);
			System.out.println("Enter the '1' to continue adding or press any number");
			choice=Integer.parseInt(br.readLine());
		}
	}
	int dispatch()
	{
		int choice=1;
		while(choice==1)
		{
			String name;
			System.out.println("Enter the name of the Customer's car to be dispatched");
			name=br.readLine();
			l1.remove(name);
			System.out.println("Enter the '1' to continue dispatch or press any number");
			choice=Integer.parseInt(br.readLine());
		}
		System.out.println("Remaining cars to be repaired");
		System.out.println(l1);
	}
	}
	public static void main(String ags[])throws Exception
	{
		Collections c;
		
		
}
